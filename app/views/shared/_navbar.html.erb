<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <div class="container-fluid">
      <h2 id="logo" class="btn">


    <div class="new-project">


    <a href="/"><img style="width: 50px"srcset="https://img.icons8.com/external-flatart-icons-outline-flatarticons/344/external-clock-summer-flatart-icons-outline-flatarticons.png 2x" alt="icon" loading="lazy"></a></h2>
    <div class="btn-container">
    <div><%= link_to "All Projects",  projects_path, class:"btn underline", id: "project-btn" %>
    </div>
    <div>
    <%= link_to "Create a New Project",  new_project_path, class:"btn underline", id: "project-btn-2" %>
    </div>
</div>
    </div>


    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto">
        <% if user_signed_in? %>
          <li class="nav-item dropdown">
          <% if current_user.photo.key != nil %>
            <%= cl_image_tag current_user.photo.key, class: "nav-avatar dropdown-toggle", id: "navbarDropdown", data: { bs_toggle: "dropdown" } %>

          <% else %>
            <%= image_tag "https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fmendocoastpm.com%2Fwp-content%2Fuploads%2F2015%2F06%2Fuser_avatar.png&f=1&nofb=1", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { bs_toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          <% end %>
            <div class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
              <%= link_to "Dashboard", dashboard_path, class: "dropdown-item" %>
              <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
            </div>
          </li>
        <% else %>
          <li class="nav-item">
            <%= link_to "Login", new_user_session_path, class: "nav-link" %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
